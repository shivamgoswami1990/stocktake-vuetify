{
  "name": "stocktake-vuetify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "@mdi/font": "^4.7.95",
    "actioncable": "^5.2.4-1",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.15",
    "written-number": "^0.9.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-airbnb": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "chart.js": "^2.9.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vuetify": "~1.0.0-beta.5",
    "eslint-plugin-vue": "^6.0.1",
    "sass": "^1.24.0",
    "sass-loader": "^7.3.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-vars": 0,
      "vue/no-parsing-error": 0,
      "vue/no-use-v-if-with-v-for": 0,
      "prefer-template": 0,
      "func-names": 0,
      "comma-dangle": 0,
      "no-console": 0,
      "no-shadow": 0,
      "consistent-return": 0,
      "max-len": 0,
      "no-unused-vars": 0,
      "no-plusplus": 0,
      "prefer-destructuring": 0,
      "no-unused-expressions": 0,
      "no-param-reassign": 0,
      "no-alert": 0,
      "no-restricted-globals": 0,
      "operator-assignment": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
