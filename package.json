{
  "name": "stocktake-vuetify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "@mdi/font": "^5.7.55",
    "actioncable": "^5.2.4-4",
    "vue": "^2.6.12",
    "vue-chartjs": "^3.5.1",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.4.7",
    "vuetify": "^2.3.14",
    "written-number": "^0.9.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.7",
    "@vue/cli-plugin-eslint": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "babel-eslint": "^10.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "chart.js": "^2.9.3",
    "eslint": "^7.11.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.7",
    "eslint-plugin-vue": "^7.1.0",
    "postcss-pxtorem": "^5.1.1",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/no-unused-vars": 0,
      "vue/no-parsing-error": 0,
      "vue/no-use-v-if-with-v-for": 0,
      "prefer-template": 0,
      "func-names": 0,
      "comma-dangle": 0,
      "no-console": 0,
      "no-shadow": 0,
      "consistent-return": 0,
      "max-len": 0,
      "no-unused-vars": 0,
      "no-plusplus": 0,
      "prefer-destructuring": 0,
      "no-unused-expressions": 0,
      "no-param-reassign": 0,
      "no-alert": 0,
      "no-restricted-globals": 0,
      "operator-assignment": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-pxtorem": {
        "rootValue": 15,
        "unitPrecision": 5,
        "propList": [
          "*"
        ],
        "replace": true
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
